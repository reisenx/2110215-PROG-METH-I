package logic.components;

public class Ore {
	// Class Properties
	private String name;
	private int cost;
	
	public Ore(String name, int cost) {
		
	}
	
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}

	public int getCost() {
		return cost;
	}

	public void setCost(int cost) {
		this.cost = cost;
	}
}
