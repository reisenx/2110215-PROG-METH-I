package logic.ghost;

import logic.game.GameController;
import utils.Config;

public class PryGhost extends LowGhost{
	// Class Properties
	private int power;
	private int ppt;
	
	// Constructor Method
	public PryGhost() {
		super();
		this.setPpt(0);
		
	}
	
	// Getters & Setter
	public int getPpt() {
		return this.ppt;
	}
	public void setPpt(int ppt) {
		this.ppt = ppt;
	}
}
