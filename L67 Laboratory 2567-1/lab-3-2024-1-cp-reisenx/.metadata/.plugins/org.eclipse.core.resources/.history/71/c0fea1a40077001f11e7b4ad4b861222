package logic.ghost;

import logic.game.GameController;
import utils.Config;

public class PooYaGhost extends HighGhost{
	// Class Properties
	private int power;
	
	// Constructor Method
	public PooYaGhost(int power) {
		super();
		this.power = power;
	}
	
	// Methods
	public String toString() {
		return "[HP: " + this.getHp() + " , Power: " + this.power + "]";
	}
	public void attack() {
		
	}
	
	// Getters
	public int getLevel() {
		return Config.PooYaGhostLevel;
	}
}
